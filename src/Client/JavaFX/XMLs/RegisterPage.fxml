<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="464.0" prefWidth="549.0" styleClass="background" stylesheets="@../css/LoginPage.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Client.JavaFX.RegisterController">
   <children>
      <Button layoutX="240.0" layoutY="301.0" mnemonicParsing="false" onAction="#register" styleClass="Button" stylesheets="@../css/LoginPage.css" text="Register">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <TextField fx:id="regUserName" layoutX="195.0" layoutY="189.0" promptText="Username" />
      <TextField fx:id="regEmail" layoutX="195.0" layoutY="145.0" promptText="Email" />
      <Label layoutX="222.0" layoutY="60.0" prefHeight="58.0" prefWidth="94.0" text="Register">
         <font>
            <Font name="Arial" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="regPassword" layoutX="195.0" layoutY="232.0" promptText="Password" />
      <Button fx:id="backToLogin" layoutX="226.0" layoutY="345.0" mnemonicParsing="false" onAction="#back" styleClass="Button" stylesheets="@../css/LoginPage.css" text="Back to login">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
